{
  "name": "@nativescript/plugin-tools-source",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "./scripts/package.sh --local",
    "clean": "npx rimraf package-lock.json node_modules",
    "commit": "git-cz",
    "checkcommit": "node ./scripts/commit-lint.js",
    "e2e": "./scripts/e2e.sh",
    "e2e-rerun": "./scripts/e2e-rerun.sh",
    "format": "./scripts/format.sh",
    "linknpm": "./scripts/link.sh",
    "copy": "./scripts/copy.sh",
    "test": "yarn linknpm fast && ./scripts/test_schematics.sh",
    "checkformat": "./scripts/check_format.sh",
    "debug": "node --debug-brk ./node_modules/@angular/cli/bin/ng g command",
    "publish-local": "./scripts/publish_local.sh",
    "publish-npm": "./scripts/publish.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "reset.changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "contributors": "githubcontrib --owner NativeScript --repo $(basename $(pwd)) --cols 6 --showlogin true --sortOrder desc | pbcopy"
  },
  "devDependencies": {
    "@angular-devkit/core": "~10.0.0",
    "@angular-devkit/schematics": "~10.0.0",
    "@nrwl/eslint-plugin-nx": "~10.1.0",
    "@nrwl/jest": "~10.1.0",
    "@nrwl/node": "~10.1.0",
    "@nrwl/workspace": "~10.1.0",
    "@nstudio/focus": "~10.1.1",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "~26.0.7",
    "@types/node": "~12.0.0",
    "@types/prettier": "~2.0.2",
    "@types/xml2js": "^0.4.5",
    "@types/yargs": "~15.0.5",
    "app-root-path": "~3.0.0",
    "codelyzer": "~6.0.0",
    "commitizen": "~4.1.2",
    "conventional-changelog-cli": "~2.1.0",
    "cosmiconfig": "~6.0.0",
    "cypress": "~4.11.0",
    "cz-conventional-changelog": "~3.2.0",
    "dotenv": "~8.2.0",
    "express": "~4.17.1",
    "fs-extra": "~9.0.1",
    "fx": "~19.0.1",
    "github-contributors-list": "~1.2.3",
    "glob": "~7.1.4",
    "graphviz": "~0.0.8",
    "html-webpack-plugin": "~4.3.0",
    "husky": "~4.2.5",
    "identity-obj-proxy": "~3.0.0",
    "ignore": "~5.1.8",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "~0.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jest": "~26.1.0",
    "jest-jasmine2": "~26.1.0",
    "jest-preset-angular": "~8.2.1",
    "jest-worker": "~26.1.0",
    "ng-packagr": "~10.1.0",
    "npm-run-all": "~4.1.5",
    "opn": "~6.0.0",
    "precise-commits": "~1.0.2",
    "prettier": "~2.0.0",
    "release-it": "~13.6.6",
    "rxjs": "~6.6.0",
    "semver": "~7.3.0",
    "shelljs": "~0.8.4",
    "source-map-support": "~0.5.19",
    "strip-json-comments": "~3.1.1",
    "tmp": "~0.2.1",
    "tree-kill": "~1.2.2",
    "ts-loader": "~8.0.1",
    "ts-node": "~8.10.2",
    "tsickle": "~0.38.1",
    "tslib": "~2.0.0",
    "tslint": "~6.1.2",
    "typescript": "~3.9.0",
    "verdaccio": "~4.8.0",
    "xml2js": "~0.4.23",
    "yargs": "~15.4.0",
    "yargs-parser": "~18.1.3",
    "zone.js": "~0.11.1"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "tmp",
      "<rootDir>/test",
      "<rootDir>/packages",
      "collection/.*/files"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "webpack-configs"
    ],
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn checkcommit && yarn checkformat"
    }
  }
}
